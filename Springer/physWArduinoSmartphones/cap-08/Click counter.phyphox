<phyphox xmlns="http://phyphox.org/xml" xmlns:editor="http://phyphox.org/editor/xml" version="1.7" editor:version="1.0" locale="en">
    <title>Click counter</title>
    <category>Statistics</category>
    <description>This experiment just counts sound events whose intensity is above a given threshold. It can be used to simulate a Geiger counter counting radioactive decays. Just provide a random source of clicks and count them.</description>
    <icon format="base64"></icon>
    <data-containers>
        <container size="19200" static="false">out</container>
        <container size="1000" static="false">stddev</container>
        <container size="1000" static="false">output</container>
        <container size="1000" static="false">result</container>
        <container size="1000" static="false">count</container>
        <container size="1000" static="false">out (1)</container>
        <container size="1000" static="false">quotient</container>
    </data-containers>
    <input>
        <audio editor:uuid="101" editor:posx="115.2118034362793" editor:posy="414.9479293823242" rate="48000">
            <output>out</output>
        </audio>
    </input>
    <output>
    </output>
    <analysis sleep="0.1"  onUserInput="false">
        <timer editor:uuid="149" editor:posx="122.0833387374878" editor:posy="713.3333511352539">
            <output as="out" clear="false">out (1)</output>
        </timer>
        <average editor:uuid="150" editor:posx="347.4444532394409" editor:posy="416.8263931274414">
            <input as="buffer" clear="false">out</input>
            <output as="stddev" clear="false">stddev</output>
        </average>
        <if editor:uuid="151" editor:posx="602.836817741394" editor:posy="379.10765838623047" less="false" equal="false" greater="true">
            <input as="a" clear="false">stddev</input>
            <input as="b" clear="false">output</input>
            <input as="true" type="value">1</input>
            <output as="result" clear="false">result</output>
        </if>
        <count editor:uuid="152" editor:posx="893.2639417648315" editor:posy="374.8993148803711">
            <input as="buffer" clear="false">result</input>
            <output as="count">count</output>
        </count>
        <divide editor:uuid="153" editor:posx="1184.0104398727417" editor:posy="594.7291946411133">
            <input as="dividend" clear="false">count</input>
            <input as="divisor" clear="false">out (1)</input>
            <output as="quotient">quotient</output>
        </divide>
    </analysis>
    <views>
        <view label="RESULTS">
            <graph editor:uuid="160" editor:posx="1607.638918876648" editor:posy="430.05904388427734" label="RMS history" aspectRatio="2.5" style="vbars" lineWidth="1" color="ff7e22" partialUpdate="true" history="1" labelX="time" labelY="Intensity" labelZ="" unitX="s" unitY="a.u." unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="fixed" scaleMaxX="auto" scaleMinY="extend" scaleMaxY="extend" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0"  mapWidth="0"       >
                <input axis="x">out (1)</input>
                <input axis="y">stddev</input>
            </graph>
            <edit editor:uuid="161" editor:posx="108.09722423553467" editor:posy="194.84375095367432" label="Threshold" signed="true" decimal="true"   unit="a.u." factor="1" default="0.1">
                <output>output</output>
            </edit>
            <value editor:uuid="162" editor:posx="1181.4653577804565" editor:posy="372.4444808959961" label="Clicks" size="3" precision="0" scientific="false"  factor="1" >
                <input>count</input>
            </value>
            <value editor:uuid="163" editor:posx="1438.0034818649292" editor:posy="594.8055801391602" label="rate" size="2" precision="2" scientific="false" unit="Hz" factor="1" >
                <input>quotient</input>
            </value>
            <value editor:uuid="164" editor:posx="1193.1459379196167" editor:posy="787.5868453979492" label="Time" size="1" precision="3" scientific="false" unit="s" factor="1" >
                <input>out (1)</input>
            </value>
        </view>
    </views>
    <export>
    </export>
</phyphox>
